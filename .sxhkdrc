alt + space
    rofi -show run
alt + c
    tdrop -w -4 -y 300 -s dropdown xterm
alt + t
    bspc desktop -l next
alt + tab
    rofi -show run
ctrl + {_,shift + } tab
    bspc node -f {next,prev}.local
ctrl + {left,right}
    bspc desktop -f {next,prev}
ctrl + {1,2,3,4}
    bspc desktop -f {1,2,3,4}
